{
  "name": "commie-koop",
  "version": "0.0.3",
  "description": "LIbrary for building koops for Commie",
  "keywords": [],
  "license": "WTFPL",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/main.d.ts",
      "require": "./lib/main.js",
      "import": "./lib/main.mjs"
    }
  },
  "main": "lib/main.js",
  "module": "lib/main.mjs",
  "types": "lib/main.d.ts",
  "bin": {
    "koop-example-executable": "lib/main.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "xo",
    "lint:fix": "xo --fix"
  },
  "xo": {
    "ignores": [
      "lib/**"
    ],
    "rules": {
      "@typescript-eslint/naming-convention": "off"
    },
    "space": true
  },
  "dependencies": {
    "dedent": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "xo": "^0.60.0"
  }
}
